#  MY_PROJECT_INCLUDE_DIRS - include directories
#  MY_PROJECT_LIBRARIES    - libraries to link against

get_filename_component(MY_PROJECT_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

if(TARGET my_project::calculator)
else()
  message(STATUS "my-project-config.cmake including ${MY_PROJECT_CMAKE_DIR}/my-project-targets.cmake")
  include("${MY_PROJECT_CMAKE_DIR}/my-project-targets.cmake")
endif()

set(MY_PROJECT_LIBRARIES my_project::calculator)

message(STATUS "my-project-config.cmake in ${CMAKE_CURRENT_LIST_FILE}")
message(STATUS "MY_PROJECT_LIBRARIES ${MY_PROJECT_LIBRARIES}")
